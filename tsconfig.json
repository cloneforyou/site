{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "esnext",
		"allowJs": true,
		"checkJs": true,
		"jsx": "react",
		"esModuleInterop": true,
		"lib": ["dom", "es2015", "es2017", "esnext.asynciterable"],
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["./src/**/*"]
}
