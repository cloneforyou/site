---
title: Notas
layout: default.liquid
tags: []
collection: notes
mdEntryType: blogPost
mainClass: collection notes
htmlClass: h-feed
sitemapChangefreq: daily
sitemapPriority: 1
pagination:
    data: collections.notes
    alias: entries
    size: 10
---

{% extends 'templates/collection.njk' %}
{% from 'macros/wm-description.njk' import wmDescriptions %}

{% block css %}
	<link rel="stylesheet" href="/css/collection-notes.css"/>
{% endblock %}

{% block entry %}

	<section>{{ entry.templateContent | safe }}</section>
	<header>
        â€” <a class="u-url" href="{{ entry.url }}">
			{% if entry.data.date %}
				<time
                    itemprop="datePublished dateModified pubdate"
                    class="dt-published"
                    datetime="{{ entry.data.date | formatDateISO }}"
                >
					{{ entry.data.date | formatDateTime }}
				</time>
			{% endif %}
		</a>
		{{ wmDescriptions(entry.data.links) }}

	</header>

{% endblock %}

<!-- this needs to be here for pagination to work -->